<header>
    <a href="#"> <img id="logo" src="/images/logo.png" alt="Company Logo"></a>
    <nav id="navigation">
        {% set navPages = collections.all | eleventyNavigation %}
        <ul id="menu">
            {%- for entry in navPages %}
            <li{% if entry.url == page.url %} class="active" {% endif %}>
                {% if entry.title != "Contact" %}     
                    <a href="{{ entry.url | url }}">{{ entry.title }}</a> 
                {% endif %}
            </li>
            {% if entry.title == "Contact" %}     
                <a href="{{ entry.url | url }}"><button{% if entry.url == page.url %} class="btactive" {% else %} class="normal" {% endif %}>{{ entry.title }}</button></a> 
            {% endif %}
            {%- endfor %}
        </ul>
    </nav>
</header>